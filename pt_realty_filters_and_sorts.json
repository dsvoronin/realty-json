{
    "filters": [
        {
            "title": "Тип объявления",
            "type": "LIST",
            "alias": "isSell",
            "data": {
                "singleOf": {
                    "param": "isSell",
                    "source": [
                        {
                            "title": "Продажа",
                            "value": true
                        },
                        {
                            "title": "Аренда",
                            "value": false
                        }
                    ]
                }
            },
            "value": true,
            "visible-condition": true
        },
        {
            "title": "Тип объекта",
            "type": "GROUPPEDLIST",
            "alias": "objectType",
            "data": {
                "singleOf": {
                    "param": "objectType",
                    "source": [
                        {
                            "title": "Квартиры",
                            "value": "flat",
                            "group": {
                                "title": "Жилая",
                                "value": "living"
                            }
                        },
                        {
                            "title": "Комнаты",
                            "value": "room",
                            "group": {
                                "title": "Жилая",
                                "value": "living"
                            }
                        },
                        {
                            "title": "Дома",
                            "value": "house",
                            "group": {
                                "title": "Жилая",
                                "value": "suburban"
                            }
                        },
                        {
                            "title": "Коттеджи",
                            "value": "cottage",
                            "group": {
                                "title": "Загородная",
                                "value": "suburban"
                            }
                        },
                        {
                            "title": "Таунхаусы",
                            "value": "townhouse",
                            "group": {
                                "title": "Загородная",
                                "value": "suburban"
                            }
                        },
                        {
                            "title": "Дачи",
                            "value": "summerHouse",
                            "group": {
                                "title": "Загородная",
                                "value": "suburban"
                            }
                        },
                        {
                            "title": "Участки",
                            "value": "land",
                            "group": {
                                "title": "Загородная",
                                "value": "land"
                            },
                            "visible-condition": {
                                "isSell.value": true
                            }
                        },
                        {
                            "title": "Магазины",
                            "value": "shop",
                            "group": {
                                "title": "Коммерческая",
                                "value": "commercial"
                            }
                        },
                        {
                            "title": "Отели",
                            "value": "hotel",
                            "group": {
                                "title": "Коммерческая",
                                "value": "commercial"
                            }
                        },
                        {
                            "title": "Офисы",
                            "value": "office",
                            "group": {
                                "title": "Коммерческая",
                                "value": "commercial"
                            }
                        },
                        {
                            "title": "Производства",
                            "value": "manufacture",
                            "group": {
                                "title": "Коммерческая",
                                "value": "commercial"
                            }
                        },
                        {
                            "title": "Склады",
                            "value": "warehouse",
                            "group": {
                                "title": "Коммерческая",
                                "value": "commercial"
                            }
                        },
                        {
                            "title": "Свободного назначения",
                            "value": "free",
                            "group": {
                                "title": "Коммерческая",
                                "value": "commercial"
                            }
                        }
                    ]
                }
            },
            "value": "flat",
            "visible-condition": true
        },
        {
            "title": "Локация",
            "type": "LOCATION",
            "alias": "location",
            "data": {
                "city": {
                    "param": "cityId",
                    "value": "city.id"
                },
                "district": {
                    "param": "districtId",
                    "value": "district.id"
                },
                "metro": {
                    "param": "metroId",
                    "value": "metro.id"
                },
                "api": "/rest/realtyOffer/getFilters/"
            },
            "visible-condition": true
        },
        {
            "title": "Цена",
            "type": "PRICE_RANGE",
            "alias": "priceRange",
            "data": {
                "min": {
                    "param": "price.min"
                },
                "max": {
                    "param": "price.max"
                },
                "currency": {
                    "param": "priceCurrency",
                    "source": [
                        {
                            "symbol": "₽",
                            "title": "Рубли",
                            "value": "rub"
                        },
                        {
                            "symbol": "$",
                            "title": "Доллары США",
                            "value": "usd"
                        },
                        {
                            "symbol": "€",
                            "title": "Евро",
                            "value": "eur"
                        }
                    ]
                }
            },
            "visible-condition": true
        },
        {
            "title": "Комнат",
            "type": "LIST",
            "multiselection": true,
            "alias": "roomsTotal",
            "data": {
                "singleOf": {
                    "param": "roomsTotal",
                    "source": [
                        {
                            "title": "1",
                            "value": "1"
                        },
                        {
                            "title": "2",
                            "value": "2"
                        },
                        {
                            "title": "3",
                            "value": "3"
                        },
                        {
                            "title": "4",
                            "value": "4"
                        },
                        {
                            "title": "5",
                            "value": "5"
                        },
                        {
                            "title": "6",
                            "value": "6",
                            "visible-condition": {
                                "objectType.group.value": "suburban"
                            }
                        },
                        {
                            "title": "6+",
                            "value": "6+"
                        },
                        {
                            "title": "Студия",
                            "value": "studio",
                            "visible-condition": {
                                "$not": {
                                    "objectType.value": "room"
                                }
                            }
                        }
                    ]
                }
            },
            "visible-condition": {
                "$and": [
                    {
                        "objectType.group.value": {
                            "$in": [
                                "living",
                                "suburban"
                            ]
                        }
                    },
                    {
                        "$not": {
                            "objectType.value": "room"
                        }
                    }
                ]
            }
        },
        {
            "title": "Комнат в аренду",
            "type": "LIST",
            "alias": "roomsOffered",
            "data": {
                "singleOf": {
                    "param": "roomsOffered",
                    "source": [
                        {
                            "title": "1",
                            "value": "1"
                        },
                        {
                            "title": "2",
                            "value": "2"
                        },
                        {
                            "title": "3",
                            "value": "3"
                        },
                        {
                            "title": "4",
                            "value": "4"
                        },
                        {
                            "title": "5",
                            "value": "5"
                        },
                        {
                            "title": "6",
                            "value": "6",
                            "visible-condition": {
                                "objectType.group.value": "suburban"
                            }
                        }
                    ]
                }
            },
            "visible-condition": {
                "$and": [
                    {
                        "isSell.value": false
                    },
                    {
                        "objectType.value": "room"
                    }
                ]
            }
        },
        {
            "title": "Участок, сотки",
            "type": "SPACE_RANGE",
            "alias": "landSpaceRange",
            "data": {
                "from": {
                    "param": "landSpace.min"
                },
                "to": {
                    "param": "landSpace.max"
                },
                "unit": {
                    "param": "landSpaceUnit",
                    "source": [
                        {
                            "title": "соток",
                            "value": "are"
                        },
                        {
                            "title": "гектар",
                            "value": "hectare"
                        }
                    ]
                }
            },
            "visible-condition": {
                "objectType.group.value": "suburban"
            }
        },
        {
            "title": "Тип участка",
            "type": "LIST",
            "alias": "landType",
            "data": {
                "singleOf": {
                    "param": "landType",
                    "source": [
                        {
                            "title": "ИЖС",
                            "value": "ijs"
                        },
                        {
                            "title": "СНТ",
                            "value": "snt"
                        },
                        {
                            "title": "Промназначение",
                            "value": "promzone"
                        }
                    ]
                }
            },
            "visible-condition": {
                "objectType.value": "land"
            }
        }
    ],
    "sortings": [
        {
            "title": "Дата создания",
            "sortField": "createdAt",
            "visible-condition": true
        },
        {
            "title": "Цена",
            "sortField": "price",
            "visible-condition": true
        },
        {
            "title": "Комнат",
            "sortField": "roomsTotal",
            "visible-condition": {
                "$and": [
                    {
                        "objectType.group.value": {
                            "$in": [
                                "living",
                                "suburban"
                            ]
                        }
                    },
                    {
                        "$not": {
                            "objectType.value": "room"
                        }
                    }
                ]
            }
        },
        {
            "title": "Комнат",
            "sortField": "roomsOffered",
            "visible-condition": {
                "objectType.value": "room"
            }
        },
        {
            "title": "Площадь",
            "sortField": "totalSpace",
            "visible-condition": true
        },
        {
            "title": "Этаж",
            "sortField": "objectFloor",
            "visible-condition": {
                "objectType.group.value": "living"
            }
        },
        {
            "title": "Участок, сотки",
            "sortField": "landSpace",
            "visible-condition": {
                "objectType.group.value": "suburban"
            }
        },
        {
            "title": "Площадь соток",
            "sortField": "landSpace",
            "visible-condition": {
                "objectType.value": "land"
            }
        },
        {
            "title": "Тип участка",
            "sortField": "landSpace",
            "visible-condition": {
                "objectType.value": "land"
            }
        },
        {
            "title": "Класс",
            "sortField": "buildingClass",
            "visible-condition": {
                "objectType.value": {
                    "$in": [
                        "office",
                        "warehouse"
                    ]
                }
            }
        }
    ]
}