      "visible-condition": true
    },
    {
      "title": "Локация",
      "type": "LOCATION",
      "alias": "location",
      "data": {
        "city": {
          "param": "cityId",
          "value": "location.city.id"
        },
        "district": {
          "param": "districtId",
          "value": "location.district.id"
        },
        "metro": {
          "param": "metroId",
          "value": "location.metro.id"
        },
        "dataSource": {
          "api": "/rest/realtyOffer/getFilters/"
        }
      },
      "visible-condition": true
    },
    {
      "title": "Цена",
      "type": "PRICE_RANGE",
      "alias": "priceRange",
      "data": {
        "priceRange": {
          "param": "price",
          "xpath": "priceRange.range"
        },
        "currency": {
          "param": "priceCurrency",
          "xpath": "priceRange.currency.value",
          "source": [
            {
              "symbol": "₽",
              "title": "Рубли",
              "value": "rub"
            },
            {
              "symbol": "$",
              "title": "Доллары США",
              "value": "usd"
            },
            {
              "symbol": "€",
              "title": "Евро",
              "value": "eur"
            }
          ]
        }
      },
      "visible-condition": true
    }
