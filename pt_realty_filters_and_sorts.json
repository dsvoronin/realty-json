    	"singleOf": {
      		"param": "memberId",
      		"default-value": {
        		"title": "Любой"
    		}
      	},
    	"dataSource": {
        	"api": "api/mobile/employee/list/1/"
    	}
    },
    "visible-condition": {
      "isCompany": true
    }
  },
  {
    "title": "Валюта",
