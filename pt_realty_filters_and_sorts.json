      "extraParam":"locationType",
      "param": "locationId",
      "city": {
        "extraValue":"city",
        "value": "location.city.id"
      },
      "district": {
        "extraValue":"district",
        "value": "location.district.id"
      },
      "metro": {
        "extraValue":"metro",
        "value": "location.metro.id"
      },
      "dataSource": {
        "api": "/rest/realtyOffer/getFilters/"
      }
