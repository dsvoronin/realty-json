  {
    "title": "Город",
    "data": {
      "singleOf": {
        "param": "cityId",
        "api": "fias/city?regionId=${region.id}&type=city",
        "value": "city.id"
      }
    },
    "alias": "city",
    "type": "FIAS_CITY",
    "required-condition": true,
    "visible-condition": true
  },
  {
    "title": "Метро",
    "alias": "metro",
    "data": {
      "singleOf": {
        "param": "metroId",
        "api": "fias/metro?cityId=${city.id}"
      }
    },
    "type": "FIAS_METRO",
    "required-condition": true,
    "visible-condition": {
      "$and": [
        {
          "city.hasMetro": true
        },
        {
          "objectType.group.value": {
            "$in": [
              "living",
              "commercial"
            ]
          }
        }
      ]
    }
  },
  {
    "title": "Район",
    "alias": "district",
