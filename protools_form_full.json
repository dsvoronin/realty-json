      "singleOf": {
        "param": "districtId",
        "api": "fias/district?cityId=${city.id}"
      }
    },
    "type": "FIAS_DISTRICT",
    "required-condition": true,
    "visible-condition": {
      "$and": [
        {
          "city.hasMetro": false
        },
        {
          "city.hasDistrict": true
        },
        {
          "objectType.group.value": {
            "$in": [
              "living",
              "commercial"
            ]
          }
        }
      ]
    }
  },
  {
