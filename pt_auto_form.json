[
  {
    "title": "Тип автомобиля",
    "type": "LIST",
    "alias": "vehicleType",
    "data": {
      "singleOf": {
        "param": "vehicleType",
        "source": [
          {
            "title": "Новый",
            "value": "newcar"
          },
          {
            "title": "С пробегом",
            "value": "usedcar"
          }
        ],
        "xpath": "vehicleType.value",
        "default-value": {
          "title": "С пробегом",
          "value": "usedcar"
        }
      }
    },
    "visible-condition": true,
    "required-condition": true,
    "editable-condition": false
  },
  {
    "title": "Автомобиль",
    "type": "HEADER",
    "icon": "car"
  },
  {
    "title": "Марка",
    "type": "STRING",
    "alias": "brand",
    "data": {
      "singleOf": {
        "param": "brandId",
        "xpath": "brand.id",
        "api": "autoOffer/getBrands"
      }
    },
    "required-condition": true,
    "visible-condition": true,
    "editable-condition": false
  },
  {
    "title": "Модель",
    "type": "STRING",
    "alias": "model",
    "data": {
      "singleOf": {
        "param": "modelId",
        "xpath": "model.id",
        "api": "autoOffer/getModels?brandId=${brand.id}"
      }
    },
    "required-condition": true,
    "visible-condition": {
      "brand.hasModels": true
    },
    "editable-condition": false,
    "clear-triggers": [
      "brand"
    ]
  },
  {
    "title": "Год выпуска",
    "type": "STRING",
    "alias": "year",
    "data": {
      "singleOf": {
        "param": "year",
        "xpath": "year.id",
        "api": "autoOffer/years"
      }
    },
    "required-condition": true,
    "visible-condition": true,
    "editable-condition": {
      "isFromAutoupload": false
    }
  },
  {
    "title": "Пробег, км",
    "type": "INTEGER",
    "alias": "mileage",
    "data": {
      "singleOf": {
        "param": "mileage",
        "xpath": "mileage"
      }
    },
    "required-condition": true,
    "visible-condition": {
      "vehicleType.value": "usedcar" 
    },
    "editable-condition": {
      "isFromAutoupload": false
    },
    "valid-conditions": [
      {
        "condition": {
          "$and": [
            {
              "mileage": {
                "$gt": 0
              }
            },
            {
              "mileage": {
                "$lt": 100000000
              }
            },
            {
              "mileage": {
                "$type": "integer"
              }
            }
          ]
        },
        "errorMessage": "Введите корректное значение"
      }
    ],
    "clear-triggers": [
      "vehicleType"
    ]
  }, 
  {
    "title": "Состояние",
    "type": "LIST",
    "alias": "condition",
    "data": {
      "singleOf": {
        "param": "condition",
        "source": [
          {
            "title": "Не битый",
            "value": "undamaged"
          },
          {
            "title": "Битый",
            "value": "damaged"
          }
        ],
        "xpath": "condition.value"
      }
    },
    "required-condition": false,
    "visible-condition": {
      "vehicleType.value": "usedcar" 
    },
    "editable-condition": {
      "isFromAutoupload": false
    },
    "clear-triggers": [
      "vehicleType"
    ]
  },
  {
    "title": "VIN",
    "type": "STRING",
    "alias": "vin",
    "data": {
      "singleOf": {
        "param": "vin",
        "xpath": "vin"
      }
    },
    "required-condition": false,
    "visible-condition": true,
    "valid-conditions": [
      {
        "condition": {
          "$and": [
            {
              "vin": {
                "$regexp":"^[A-Z0-9]{17}$"
              }
            }
          ]
        },
        "errorMessage": "Введите корректный VIN-номер: 17 символов, вместо букв I, O, Q используйте цифры 1 и 0"
      }
    ]
  },
  {
    "title": "Номер свидетельства ТС",
    "type": "STRING",
    "alias": "sts",
    "data": {
      "singleOf": {
        "param": "sts",
        "xpath": "sts"
      }
    },
    "required-condition": false,
    "visible-condition": true,
    "valid-conditions": [
      {
        "condition": {
          "$and": [
            {
              "sts": {
                "$regexp": "^[0-9]{2}([А-Я]{2}|[0-9]{2})[0-9]{6}$"
              }
            }
          ]
        },
        "errorMessage": "Введите корректный Номер свидетельства ТС: 10 символов, образец: 12АА123456 (при написании используется кириллица) или 1200123456"
      }
    ]
  },
